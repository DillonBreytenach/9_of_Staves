<Home>:
    MDFloatLayout:
        orientation: 'vertical'

        Image:
            pos_hint: {'x': 0.0,'y': -0.2}
            size_hint: 1.5, 1.5
            source: "ASSETS/BACKGROUND.jpeg"
            allow_stretch: True


        Button:
            text:'9 of Staves'
            font_size: 29
            background_normal: 'ASSETS/wood1.jpeg'
            color: (0, 0, 0, 1)
            size_hint: .8, .1
            pos_hint: {'x': 0.105,'y': 0.9}
            on_press: root.LR()

        Button:
            id: Icon_
            text:'ICON'
            size_hint: .25, .2
            pos_hint: {'x': 0.105,'y': 0.6}
            on_press: root.on_start()

        Button:
            id: Player
            text:'PLAYER'
            size_hint: .6, .2
            pos_hint: {'x': 0.355,'y': 0.6}
            on_press: root.on_start()

        Button:
            text: "START"
            background_normal: "ASSETS/wood2.jpeg"
            font_size: 25
            color: (210/255, 151/255, 32/255, 1)#USE COLOR PICKER
            size_hint: .8, .2
            pos_hint: {'x': 0.105,'y': 0.3}# 0.2, .4
            on_press: root.move()

        Button:
            id: spc
            background_normal: "ASSETS/wood2.jpeg"
            font_size: 25
            color: (0, 0, 0, 1)
            text: "SP_CARDS"
            size_hint: .8, .20
            pos_hint: {'x': 0.105,'y': 0.01}
            on_press: root.ads()

        Button:
            text: "TEST_RECYLE"
            size_hint: .5, .5
            pos_hint: {'x': 0.0,'y': 0.0}
            on_press: root.test_recyle()

<Register>:
    MDFloatLayout:
